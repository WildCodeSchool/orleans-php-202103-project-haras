{% block contact_grey_block %}
                    <div class="grey-block col-xl-3 offset-xl-8 col-sm-10 offset-sm-1">
                        <div class="contact-grey-section">
                        <div class="mb-3">
                                <label class="form-label">Nom</label>
                                <input type="nom" class="form-control" >
                            </div>
                        <div class="mb-3">
                                <label class="form-label">Prenom</label>
                                <input type="prenom" class="form-control" >
                            </div>
                        <div class="mb-3">
                                <label class="form-label">Telephone</label>
                                <input type="Telephone" class="form-control" >
                            </div>
                            <div class="mb-3">
                                <label class="form-label">Email</label>
                                <input type="email" class="form-control" >
                            </div>
                            <div class="mb-3">
                                <label class="form-label">Message</label>
                                <textarea class="form-control" rows="3"></textarea>
                            </div>
                        </div>
                    </div>
                {% endblock %}
